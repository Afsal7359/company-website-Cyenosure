<!-- PAGE -->
<div class="page">
    <div class="page-main">

    

        <!--app-content open-->
        <div class="app-content main-content mt-0"  >
            <div class="side-app">

                <!-- CONTAINER -->
                <div class="main-container container-fluid">

                        
                  

                    <!-- ROW-1 OPEN -->
                    <div clss="row">
                        <div clas="col-lg-6 col-md-12">
                            <div class="card">
                                <div class="card-header border-bottom">
                                    <h3 class="card-title">Dashboard</h3>
                                </div>
                                <div class="card-body">
                                  
                   
                        <div class="row">
                            <div class="row row-sm">
                                <div class="col-lg-12">
                                    <div class="card">
                                        <div class="card-header border-bottom">
                                            <h3 class="card-title"><!-- Button to toggle table visibility -->
                                               
                                                <button id="viewenquiry" class="btn btn-primary">View Enquiries</button>

                                                </h3>
                                        </div>
                                       

                                        <%if (data) { %>
                                            <div id="enquiryTable" style="display: none;">
                                                
                                                    <div class="card-header border-bottom">
                                                        <h3 class="card-title"> Enquiry  Datatable</h3>
                                                    </div>
                                                    <div class="card-body">
                                                                <div class="table-responsive">
                                                                    <table id="editable-responsive-table" class="table editable-table table-nowrap table-bordered table-edit wp-100">
                                                                        <thead>
                                                                            <tr>
                                                                                <th>ID</th>
                                                                                <th>name</th>
                                                                                <th>email</th>
                                                                                <th>number</th>
                                                                                <th>message</th>
                                                                                <th>Action</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody>
                                                                            <% for (let i = 0; i < data.length; i++) { %>
                                                                                <% let currentdata = data[i]; %>
                                                                        
                                                                            <tr>
                                                                               
                                                                                    <td><%= i + 1 %></td>
                                                                                    <td><%= currentdata.name%></td>
                                                                                    <td><%= currentdata.email%></td>
                                                                                    <td><%= currentdata.number%></td>
                                                                                    <td><%= currentdata.message%></td>
                                                                                    
                                                                                    <td>
                                                                                        <div class="card-body">
                                                                                            <p class="text-muted"></p>
                                                                                           <a  class="btn btn-danger" data-bs-target="#modaldemo3_<%=i%>" data-bs-toggle="modal">Delete </a>
                                                                                            
                                                                                       
                                                                                        </div> 
                                                                                        
                                                                                        
                                                                                    </td>
                                                                             
                                                                            </tr>
                                                                            <% } %>
                                                                            
                                                                            
                                                                          
                                                                                   
                                                                                </td>
                                                                            </tr>
                                                                        </tbody>
                                                                        
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                            <!-- End Row -->
                                            <%}%>




                                    </div>
                                </div>
                            </div>
                        </div>
                      
                      

                     
                        
                                </div>
                            </div>
                        </div>
                     
                    </div>
                    <!-- ROW-1 CLOSED -->


           

                    
                </div>
            </div>
        </div>
            <!-- CONTAINER CLOSED -->
     </div>



    
    

</div>
<!-- page -->
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>
    // Function to toggle booking table visibility
    $("#viewcargobooking").click(function() {
        $("#bookingTable").toggle();
    });
 // Function to toggle enquiry table visibility
 $("#viewenquiry").click(function() {
        $("#enquiryTable").toggle();
    });
    
</script>